<script>
$("#inpt_search").on('focus', function () {
	$(this).parent('label').addClass('active');
	// document.getElementById("icon-mysearch").style.display="";
	$("#icon-mysearch").css("display","");
	// $("#icon-mysearch").show();
});

$("#inpt_search").on('blur', function () {
	if($(this).val().length == 0){
		$(this).parent('label').removeClass('active');
		// document.getElementById("icon-mysearch").style.display="none";
		$("#icon-mysearch").css("display","none");
		// $("#icon-mysearch").hide();
	}
});
</script>
